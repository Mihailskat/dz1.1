import pandas as pd
import numpy as np

data = {'Имя': ['Миша', 'Саша', 'Ваня', 'Соня', 'Даша','Максим', 'Ксюша', 'Катя'],
        'Пол': ['М', 'Ж', 'М', 'Ж', 'Ж', 'М', 'Ж', 'Ж'],
        'Оценка за четверть': [5, 4, 4, 4, 3, 5, 5, 3]}

A = pd.DataFrame(data)
A[:3] # выводим первые 3 строки 
A[-3:] # выводим последние 3 строки
A.to_csv('my DataFrame.csv')
